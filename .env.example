# A2A Server Configuration
APP_NAME="Progressive Document Parser A2A"
APP_VERSION="0.1.0"
LOG_LEVEL="INFO"

# LLM Configuration (choose one or more)
OPENAI_API_KEY="your-openai-api-key-here"
# GOOGLE_AI_API_KEY="your-google-ai-api-key-here"

# Default LLM Model
DEFAULT_LLM_MODEL="gpt-4"
# DEFAULT_LLM_MODEL="gemini-pro"

# Redis Configuration
REDIS_HOST="localhost"
REDIS_PORT="6379"
REDIS_DB="0"
REDIS_PASSWORD=""

# Cache TTL Settings (in seconds)
CACHE_STRUCTURE_TTL="3600"  # 1 hour
CACHE_EXTRACTION_TTL="1800"  # 30 minutes
CACHE_LLM_TTL="86400"  # 24 hours

# File Processing
MAX_FILE_SIZE_MB="100"
ALLOWED_FILE_TYPES="pdf"
UPLOAD_DIR="./uploaded_documents"

# Extraction Settings
MAX_CONTEXT_WINDOW="2000"
DEFAULT_CONFIDENCE_THRESHOLD="0.7"
MAX_RETRY_ATTEMPTS="3"

# Performance Settings
ENABLE_PARALLEL_EXTRACTION="true"
MAX_CONCURRENT_EXTRACTIONS="5"
EXTRACTION_TIMEOUT_SECONDS="300"

# Security
API_KEY_HEADER="X-API-Key"
ENABLE_API_KEY_AUTH="false"
# API_KEYS="key1,key2,key3"  # Comma-separated list

# Monitoring
ENABLE_METRICS="true"
METRICS_PORT="9090"

# Development Settings
DEBUG="false"
RELOAD="false"